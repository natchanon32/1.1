#include <iostream>
using namespace std;

int main() {
    bool a = true;    // bool: เก็บค่าความจริง true หรือ false
    int b = 20;             // int: เก็บจำนวนเต็ม
    double c = 3.14159265;   // double: เก็บเลขทศนิยมแบบความแม่นยำสู
    char d= 'A';         // char: เก็บอักขระเพียงตัวเดียว
    float e = 175.5f;    // float: เก็บเลขทศนิยมแบบความแม่นยำปานกลาง
    cout<<"a="<<a<<'
    
      
    return 0;
}
